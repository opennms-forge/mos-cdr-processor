    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Launch Starter",
            "request": "launch",
            "mainClass": "org.opennms.moscdrprocessor.Starter",
            "projectName": "moscdrprocessor",
            "args": [
                "watch",
                "-v",
                "--config", "~/projects/mos-cdr-processor/runConfig.json"
            ]
        },
        {
            "type": "java",
            "name": "Attach",
            "request": "attach"
        }
    ]
}